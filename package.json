{
  "name": "adn-aws-ec2",
  "version": "1.0.0",
  "description": "Small server for EC2 instance with PostgreSQL and Prisma",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "pm2:start": "pm2 start dist/index.js --name adn-api --env NODE_ENV=production",
    "pm2:stop": "pm2 stop adn-api",
    "pm2:restart": "pm2 restart adn-api",
    "pm2:logs": "pm2 logs adn-api",
    "pm2:status": "pm2 status",
    "pm2:delete": "pm2 delete adn-api",
    "pm2:startup": "pm2 startup && pm2 save",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:seed": "tsx prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phitonthel/adn-aws-ec2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/phitonthel/adn-aws-ec2/issues"
  },
  "homepage": "https://github.com/phitonthel/adn-aws-ec2#readme",
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "express": "^4.21.0",
    "dotenv": "^16.4.7",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.0.0",
    "prisma": "^6.0.0",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0"
  },
  "engines": {
    "node": "22.x"
  }
}